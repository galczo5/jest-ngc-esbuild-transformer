{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/jest-ngc-esbuild-transformer-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/jest-ngc-esbuild-transformer-e2e/cypress.json",
        "devServerTarget": "jest-ngc-esbuild-transformer:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "jest-ngc-esbuild-transformer:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/jest-ngc-esbuild-transformer-e2e/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["jest-ngc-esbuild-transformer"]
}
